{
  "name": "chatgpt-tool-extension",
  "version": "1.5.6",
  "description": "이 프로젝트는 Chrome 확장 프로그램(Manifest V3) 개발을 위한 템플릿입니다.",
  "main": "background.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "webpack --config webpack.config.js",
    "watch": "webpack --config webpack.config.js --watch",
    "update-tiktoken": "webpack --config webpack.config.js && copy dist\\tiktoken.bundle.js thirdParty\\",
    "build:release": "npm run build && webpack --config webpack.release.js",
    "release": "npm run build:release",
    "release:all": "npm run release && npm run release:firefox",
    "build:firefox": "webpack --config webpack.release.firefox.js",
    "release:firefox": "npm run build && npm run build:firefox",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check ."
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "copy-webpack-plugin": "^13.0.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "html-webpack-plugin": "^5.6.4",
    "prettier": "^3.3.3",
    "terser-webpack-plugin": "^5.3.14",
    "webpack": "^5.99.9",
    "webpack-cli": "^6.0.1",
    "zip-webpack-plugin": "^4.0.3"
  }
}
